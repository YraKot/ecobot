#third
    .wall3 
        position: relative;

        .graffiti3 
            position: absolute;
            top: 60px;
            left: -20%;
            z-index: 999;
            opacity: 0.1;  
            color: gray;  
            font-weight: 800;
            letter-spacing: 8px;
            font-size: 150px;

    .contact_map
        position relative
        z-index 100
        display flex
        margin-top 200px

        #side_contact
            // background #805097
            height 600px
            width 40%

            .form_content 
                float: right;
                // width: 400px;

                padding: 60px 35px 75px 120px

                h2 
                    font-size: 32px;
                    color: #3e3e3e;
                
                p 
                    font-size: 16px;
                    color: #b8b8b8;
                
                
                input[type=text] 
                    width: 100%;
                    padding: 12px 20px;
                    margin: 8px 0;
                    display: inline-block;
                    border: none;
                    border-radius: 10px;
                    background: #f5f5f5;
                    box-sizing: border-box;
                
                input[type=submit] 
                    font-size: 12px;
                    color: #fff;
                    padding: 15px 35px;
                    background: #feb23a;
                    border-radius: 30px;
                    border: none;

        #button
            width 50px
            height 600px
            background rgba(254, 178, 58, 0.7)
            position absolute
            top 0
            left 40%
            right auto
            z-index 2


            display: flex;
            justify-content: center;
            align-items: center;

            img
                width 25px


        #map_contact
            height 600px 
            width 60%
            // background #D24F9F

            #map_ecobot 
                height: 600px;
                width: 100%;  
